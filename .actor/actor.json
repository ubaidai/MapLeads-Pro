{
    "actorSpecification": 1,
    "name": "google-maps-business-scraper",
    "title": "Google Maps Business Scraper",
    "description": "Extract complete business data from Google Maps including names, addresses, phone numbers, ratings, reviews, and more. Perfect for lead generation, market research, and local SEO analysis.",
    "version": "1.0",
    "meta": {
        "templateId": "puppeteer-crawler"
    },
    "input": "./INPUT_SCHEMA.json",
    "dockerfile": "./Dockerfile",
    "readme": "./README.md",
    "categories": [
        "BUSINESS",
        "LEAD_GENERATION",
        "LOCAL_DATA"
    ],
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Business Data",
            "description": "Dataset containing scraped business information",
            "views": {
                "overview": {
                    "title": "Overview",
                    "transformation": {
                        "fields": [
                            "name",
                            "rating",
                            "reviewCount",
                            "category",
                            "address"
                        ]
                    },
                    "display": {
                        "component": "table",
                        "properties": {
                            "name": {
                                "label": "Business Name",
                                "format": "text"
                            },
                            "rating": {
                                "label": "Rating",
                                "format": "text"
                            },
                            "reviewCount": {
                                "label": "Reviews",
                                "format": "text"
                            },
                            "category": {
                                "label": "Category",
                                "format": "text"
                            },
                            "address": {
                                "label": "Address",
                                "format": "text"
                            }
                        }
                    }
                }
            }
        }
    }
}
